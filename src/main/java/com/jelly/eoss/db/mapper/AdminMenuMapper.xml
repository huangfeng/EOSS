<mapper namespace="AdminMenu">
	<!-- 生成Insert -->
        	<insert id="Insert" useGeneratedKeys="true" keyProperty="id" parameterType="com.jelly.eoss.db.entity.AdminMenu">
			insert into admin_menu (
				<trim suffixOverrides=",">
                            <if test="id != null">
							id,
                            </if>

                            <if test="pid != null">
							pid,
                            </if>

                            <if test="name != null and name != ''">
							name,
                            </if>

                            <if test="leaf != null">
							leaf,
                            </if>

                            <if test="path != null and path != ''">
							path,
                            </if>

                            <if test="lev != null">
							lev,
                            </if>

                            <if test="url != null and url != ''">
							url,
                            </if>

                            <if test="target != null and target != ''">
							target,
                            </if>

                            <if test="createDatetime != null and createDatetime != ''">
							create_datetime,
                            </if>

				</trim>
			) values (
				<trim suffixOverrides=",">
                            <if test="id != null">
                                #{id},
                            </if>
                            <if test="pid != null">
                                #{pid},
                            </if>
                            <if test="name != null and name != ''">
                                #{name},
                            </if>
                            <if test="leaf != null">
                                #{leaf},
                            </if>
                            <if test="path != null and path != ''">
                                #{path},
                            </if>
                            <if test="lev != null">
                                #{lev},
                            </if>
                            <if test="url != null and url != ''">
                                #{url},
                            </if>
                            <if test="target != null and target != ''">
                                #{target},
                            </if>
                            <if test="createDatetime != null and createDatetime != ''">
                                #{createDatetime},
                            </if>
				</trim>
			)
		</insert>

	<!-- 生成Update -->
    <!-- 注意调用该SQL前必须检查参数的正确性，否则可能会误更新 -->
	<update id="Update" parameterType="com.jelly.eoss.db.entity.AdminMenu">
		update admin_menu set 
		<trim suffixOverrides=",">
                    <if test="id != null">
                    	id = #{id},
                    </if>
                    <if test="pid != null">
                    	pid = #{pid},
                    </if>
                    <if test="name != null and name != ''">
                    	name = #{name},
                    </if>
                    <if test="leaf != null">
                    	leaf = #{leaf},
                    </if>
                    <if test="path != null and path != ''">
                    	path = #{path},
                    </if>
                    <if test="lev != null">
                    	lev = #{lev},
                    </if>
                    <if test="url != null and url != ''">
                    	url = #{url},
                    </if>
                    <if test="target != null and target != ''">
                    	target = #{target},
                    </if>
                    <if test="createDatetime != null and createDatetime != ''">
                    	create_datetime = #{createDatetime},
                    </if>
		</trim>
        where
                <if test="id != null and id != ''">
				id = #{id}
                </if>
            
	</update>

    <!-- 生成UpdateWithNull -->
    <!-- 注意调用该SQL前必须先根据主键查询出该记录所有列数据，再设置某列为null -->
    <update id="UpdateWithNull" parameterType="com.jelly.eoss.db.entity.AdminMenu">
        update admin_menu set
        <trim suffixOverrides=",">
			id = #{id},
			pid = #{pid},
			name = #{name},
			leaf = #{leaf},
			path = #{path},
			lev = #{lev},
			url = #{url},
			target = #{target},
			create_datetime = #{createDatetime},
        </trim>
        where
		id = #{id}
			
    </update>
	
	<!-- 根据表的主键生成Delete -->
		<delete id="DeleteByPk" parameterType="java.io.Serializable">
			
			delete 
			from admin_menu 
			where 
				id = #{id}
				
		</delete>
	<!-- 注意调用该SQL前必须检查参数的正确性，否则可能会误删除 -->
	<delete id="DeleteByPojo" parameterType="com.jelly.eoss.db.entity.AdminMenu">
		<!-- 请注意，该表没有主键 -->
		delete
		from admin_menu
		where
        <trim suffixOverrides="and">
                <if test="id != null">
					id = #{id} and
                </if>
                <if test="pid != null">
					pid = #{pid} and
                </if>
                <if test="name != null and name != ''">
					name = #{name} and
                </if>
                <if test="leaf != null">
					leaf = #{leaf} and
                </if>
                <if test="path != null and path != ''">
					path = #{path} and
                </if>
                <if test="lev != null">
					lev = #{lev} and
                </if>
                <if test="url != null and url != ''">
					url = #{url} and
                </if>
                <if test="target != null and target != ''">
					target = #{target} and
                </if>
                <if test="createDatetime != null and createDatetime != ''">
					create_datetime = #{createDatetime} and
                </if>
		</trim>
	</delete>

	<!-- 生成Select通用表头 -->
	<sql id="SelectColumns">
		<trim suffixOverrides=",">
				id as "id",
				pid as "pid",
				name as "name",
				leaf as "leaf",
				path as "path",
				lev as "lev",
				url as "url",
				target as "target",
				create_datetime as "createDatetime",
		</trim>
	</sql>
	
	<!-- 生成Select通用查询条件 -->
	<sql id="SelectWheres">
                <if test="id != null">
                    and id = #{id}
                </if>
                <if test="pid != null">
                    and pid = #{pid}
                </if>
                <choose>
                    <when test="likeSqlColumnSet != null and 'name' in likeSqlColumnSet">
                        and name like '%${name}%'
                    </when>
                    <otherwise>
                        <if test="name != null and name != ''">
                            and name = #{name}
                        </if>
                    </otherwise>
                </choose>
                <if test="leaf != null">
                    and leaf = #{leaf}
                </if>
                <choose>
                    <when test="likeSqlColumnSet != null and 'path' in likeSqlColumnSet">
                        and path like '%${path}%'
                    </when>
                    <otherwise>
                        <if test="path != null and path != ''">
                            and path = #{path}
                        </if>
                    </otherwise>
                </choose>
                <if test="lev != null">
                    and lev = #{lev}
                </if>
                <choose>
                    <when test="likeSqlColumnSet != null and 'url' in likeSqlColumnSet">
                        and url like '%${url}%'
                    </when>
                    <otherwise>
                        <if test="url != null and url != ''">
                            and url = #{url}
                        </if>
                    </otherwise>
                </choose>
                <choose>
                    <when test="likeSqlColumnSet != null and 'target' in likeSqlColumnSet">
                        and target like '%${target}%'
                    </when>
                    <otherwise>
                        <if test="target != null and target != ''">
                            and target = #{target}
                        </if>
                    </otherwise>
                </choose>
                <choose>
                    <when test="likeSqlColumnSet != null and 'createDatetime' in likeSqlColumnSet">
                        and create_datetime like '%${createDatetime}%'
                    </when>
                    <otherwise>
                        <if test="createDatetime != null and createDatetime != ''">
                            and create_datetime = #{createDatetime}
                        </if>
                    </otherwise>
                </choose>
	</sql>

    <sql id="SelectOrderBy">
        <if test="sortConditionSet != null and sortConditionSet.size() > 0">
            order by
            <trim suffixOverrides=",">
                <foreach collection="sortConditionSet" item="item">
                    ${item.colName} ${item.sort},
                </foreach>
            </trim>
        </if>
    </sql>
	
	<!-- 生成Select -->
	<select id="Select" parameterType="com.jelly.eoss.db.entity.AdminMenu" resultType="com.jelly.eoss.db.entity.AdminMenu">
		select
		<include refid="SelectColumns"/>
		from admin_menu
		where 1 = 1
		<include refid="SelectWheres"/>
        <include refid="SelectOrderBy"/>
	</select>

    <!-- 生成Select -->
    <select id="selectPage" parameterType="com.jelly.eoss.db.entity.AdminMenu" resultType="com.jelly.eoss.db.entity.AdminMenu">
        select
        <include refid="SelectColumns"/>
        from admin_menu
        where 1 = 1
        <include refid="SelectWheres"/>
        <include refid="SelectOrderBy"/>
        limit #{offset},#{length}
    </select>

    <!-- 生成SelectCount -->
    <select id="SelectCount" parameterType="com.jelly.eoss.db.entity.AdminMenu" resultType="Long">
        select count(*) ct
        from admin_menu
        <where>
            <include refid="SelectWheres"/>
        </where>
    </select>

    <!-- 根据表的主键生成SelectByPk，该表只有单列主键 -->
	<select id="SelectByPk" parameterType="java.io.Serializable" resultType="com.jelly.eoss.db.entity.AdminMenu">
		select
		<include refid="SelectColumns"/>
		from admin_menu
		where
				id = #{id}
			
	</select>
</mapper>